{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "ayman-elmalah/laravel-myfatoorah": "^1.0",
        "beyondcode/laravel-websockets": "^1.14",
        "codebyray/laravel-review-rateable": "*",
        "creativeorange/gravatar": "^1.0",
        "cybercog/laravel-ban": "^4.7",
        "fakerphp/faker": "^1.20",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "garygreen/pretty-routes": "^1.0",
        "giggsey/libphonenumber-for-php": "^8.13",
        "grimzy/laravel-mysql-spatial": "*",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.0",
        "laravel/sanctum": "^2.15",
        "laravel/tinker": "^2.0",
        "laravolt/avatar": "^5.0",
        "munafio/chatify": "^1.6",
        "pusher/pusher-php-server": "7.0.2",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-ignition": "^1.6",
        "spatie/laravel-json-api-paginate": "^1.13",
        "spatie/laravel-medialibrary": "^9.12",
        "spatie/laravel-permission": "^6.3",
        "spatie/laravel-query-builder": "^4.0",
        "spatie/laravel-settings": "^3.2",
        "tightenco/ziggy": "^1.8",
        "twilio/sdk": "^7.14"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "facade/ignition": "^2.3.6",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-web-tinker": "^1.8"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/Helpers.php",
            "app/Helpers/Myfatorah.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
